
<h2>Mechanic:</h2>
<ul>
  @if (Model.Count != 0)
  {
    @foreach (var Mechanic in Model)
    {
      <li><a href="/Mechanics/@Mechanic.GetId()">@Mechanic.GetName()</a>
       | <a href="/Mechanic/edit/@Mechanic.GetId()">Edit</a>
       | <a href="Mechanic/delete/@Mechanic.GetId()">Delete</a>
      </li>
    }
  }
</ul>
<h4><a href="/Mechanics/new">Add new Mechanic</a></h4>
<h4><a href="/">Return home</a></h4>
